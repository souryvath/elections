    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "docker:build-api": "docker build --target app-api -t app-api .",
    "docker:build-front": "docker build --target app-ui -t app-ui .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "dev:ssr": "ng run sample:serve-ssr",
    "serve:ssr": "node dist/apps/sample/server/main.js",
    "build:ssr": "ng build --configuration production && ng run sample:server:production",
    "build:ssr:dev": "ng build --configuration production && ng run sample:server:production",
    "prerender": "ng run sample:prerender",
    "workspace-generator": "nx workspace-generator",
    "clear:cache": "ng config cli.cache.enabled false",
    "pwa": "pwa-asset-generator logo.png ./apps/sample/src/assets/icons -c  -i ./apps/sample/src/index.html -m ./apps/sample/src/manifest.webmanifest",
    "deph:graph": "nx dep-graph --file dep-graph.json"
  },
